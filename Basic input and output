// 1. WAP to calculate average of three integers. Numbers are given by the user

#include<stdio.h>
int main()
{
    int a,b,c;
    float avg;
    printf("Enter three integers:\n");
    scanf("%d %d %d",&a,&b,&c);

    avg = (a + b + c) / 3.0;    // 3.0 is bcz int/float = float

    printf("The average of three integers is %f",avg);
    return 0;
}


